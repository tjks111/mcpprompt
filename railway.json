{
  "$schema": "https://railway.app/railway.schema.json",
  "services": {
    "claude-prompts-mcp": {
      "root": "server",
      "build": {
        "builder": "NIXPACKS"
      },
      "deploy": {
        "numReplicas": 1,
        "startCommand": "npm run start",
        "healthcheckPath": "/",
        "healthcheckTimeout": 300
      },
      "environment": {
        "NODE_ENV": "production",
        "PORT": "3000"
      },
      "plugins": [],
      "variables": {}
    }
  }
}
